{
    "source" : {
        "include" : ["js/"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "buildInURL": "https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Global_Objects/",
        "buildins": [
          "array",
          "arraybuffer",
          "boolean",
          "date",
          "error",
          "function",
          "json",
          "number",
          "object",
          "regexp",
          "string",
          "null"
        ],
        "smallSourceLink": true,
        "hideAuthor": false,
        "default": {
            "outputSourceFiles": true
        }        
      },
    "extendedConfig": {        
    },
    "opts" : {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs-js/",
        "recurse": true,
        "verbose": true,
        "template": "./node_modules/minami",
        "theme_opts": {            
            "theme": "light"
        }
    }
}
